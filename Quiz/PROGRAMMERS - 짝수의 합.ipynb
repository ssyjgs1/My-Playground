    "def solution(n):\n",
    "    sum = 0\n",
    "    for i in range(1, n+1) :\n",
    "        if n % 2 == 0 :\n",
    "            sum += n\n",
    "        n = n - 1\n",
    "    answer = sum\n",
    "    return answer"
   ]
